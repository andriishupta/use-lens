{
  "name": "@use-lens/cli",
  "version": "0.0.4",
  "description": "CLI for Lens API generation",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/andriishupta/use-lens",
  "author": "Andrii Shupta <andrii.shupta@gmail.com>",
  "license": "MIT",
  "private": false,
  "bin": {
    "use-lens": "./lib/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "api",
    "lib",
    ".use-lens.json",
    "lens-api.documents.graphql",
    "lens-api.schema.graphql",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "precompile": "cp ../../api/{lens-api.schema.graphql,lens-api.documents.graphql,.use-lens.json} ./api && cp ../react-apollo/codegen.yml ./api/react-apollo.yml",
    "compile": "tsc --project tsconfig.json",
    "build": "yarn run compile"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^2.12.0",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "chalk": "^5.0.1",
    "commander": "^9.4.0"
  },
  "gitHead": "65a9b977a5bb0f1da2c3d0622df1a8b9bcc12a73"
}
