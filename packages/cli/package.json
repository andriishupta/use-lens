{
  "name": "@use-lens/cli",
  "version": "0.0.1-alpha.8",
  "description": "CLI for generation Lens API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/andriishupta/use-lens",
  "author": "Andrii Shupta <andrii.shupta@gmail.com>",
  "license": "MIT",
  "private": false,
  "bin": {
    "use-lens": "./lib/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    ".use-lens.json",
    "lens-api.documents.graphql",
    "lens-api.schema.graphql",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "precompile": "cp ../../{lens-api.schema.graphql,lens-api.documents.graphql,.use-lens.json} .",
    "compile": "tsc --project tsconfig.json",
    "build": "npm run compile"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "commander": "^9.4.0",
    "node-fetch": "^3.2.10"
  },
  "gitHead": "65a9b977a5bb0f1da2c3d0622df1a8b9bcc12a73",
  "devDependencies": {
    "@types/node-fetch": "^2.6.2"
  }
}
