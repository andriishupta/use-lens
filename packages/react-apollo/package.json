{
  "name": "@use-lens/react-apollo",
  "version": "0.0.1-alpha.8",
  "description": "Lens API for Apollo Client in React",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/andriishupta/use-lens",
  "author": "Andrii Shupta <andrii.shupta@gmail.com>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    ".use-lens.json",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "pregenerate": "cp ../../{lens-api.schema.graphql,lens-api.documents.graphql,.use-lens.json} .",
    "generate": "graphql-codegen",
    "compile": "tsc --project tsconfig.json",
    "build": "npm run generate && npm run compile"
  },
  "devDependencies": {
    "@graphql-codegen/typescript-react-apollo": "^3.3.3"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9"
  }
}
